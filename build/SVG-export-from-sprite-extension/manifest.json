{
	"manifest_version": 3,
	"name": "SVG Export From Sprite",
	"description": "Экспорт иконок из svg-спрайта",
	"version": "1.5.3",
	"content_scripts": [{
      "matches": [
        "<all_urls>"
      ],
      "js": ["content.js"]
    }],
	"icons": {"128": "icon/icon_128.png"},
	"action": {
		"default_icon": "icon/icon_128.png",
		"default_popup": "index.html"
	},
	"permissions": [
		"activeTab",
		"storage",
		"tabs"
	]
}